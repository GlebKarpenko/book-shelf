{
  "name": "book-shelf-backend",
  "version": "1.0.0",
  "description": "Backend for personalized library website",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "dev": "nodemon",
    "scraper": "ts-node -r tsconfig-paths/register src/services/scraper/scraperHandler.ts",
    "score": "ts-node -r tsconfig-paths/register src/services/score/main.ts",
    "index-schema": "ts-node -r tsconfig-paths/register src/models/scripts/indexSchema.ts",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePaths": [
      "backend/src"
    ],
    "^@app/(.*)$": "backend/src/$1"
  },
  "author": "Gleb Karpenko",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/string-similarity": "^4.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@types/axios": "^0.14.4",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.8.3",
    "string-similarity": "^4.0.4",
    "string-similarity-js": "^2.1.4",
    "tsconfig-paths": "^4.2.0"
  }
}
